{
  "name": "@nextra/typescript",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "description": "TypeScript Integration for Nextra",
  "repository": "github:shuding/nextra",
  "homepage": "https://nextra.site",
  "author": "Dimitri POSTOLOV",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "next.js",
    "nextra",
    "docs",
    "typescript"
  ],
  "scripts": {
    "build": "NODE_ENV=production tsup",
    "dev": "tsup --watch",
    "types:check": "tsc --noEmit"
  },
  "peerDependencies": {
    "nextra-theme-docs": ">=4",
    "nextra": ">=4",
    "typescript": "*"
  },
  "dependencies": {
    "@sindresorhus/slugify": "^2.2.1",
    "clsx": "^2.1.1",
    "fast-glob": "^3.3.3",
    "ts-morph": "^25.0.1"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "22.13.10",
    "@types/react": "19.0.11",
    "nextra": "^4.2.17",
    "nextra-theme-docs": "^4.2.17",
    "tsup": "^8.4.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
